package com.znsd.bean;

//选项属性类
public class TopicoptionsBean {
	private Integer topicId;
	private String a;
	private String b;
	private String c;
	private String d;

	public TopicoptionsBean() {
		super();
	}

	public TopicoptionsBean(Integer topicId, String a, String b, String c, String d) {
		super();
		this.topicId = topicId;
		this.a = a;
		this.b = b;
		this.c = c;
		this.d = d;
	}
	public TopicoptionsBean( String a, String b, String c, String d) {
		super();
		
		this.a = a;
		this.b = b;
		this.c = c;
		this.d = d;
	}

	public Integer getTopicId() {
		return topicId;
	}

	public void setTopicId(Integer topicId) {
		this.topicId = topicId;
	}

	public String getA() {
		return a;
	}

	public void setA(String a) {
		this.a = a;
	}

	public String getB() {
		return b;
	}

	public void setB(String b) {
		this.b = b;
	}

	public String getC() {
		return c;
	}

	public void setC(String c) {
		this.c = c;
	}

	public String getD() {
		return d;
	}

	public void setD(String d) {
		this.d = d;
	}

	@Override
	public String toString() {
		return "TopicoptionsBean [topicId=" + topicId + ", a=" + a + ", b=" + b + ", c=" + c + ", d=" + d + "]";
	}

}
