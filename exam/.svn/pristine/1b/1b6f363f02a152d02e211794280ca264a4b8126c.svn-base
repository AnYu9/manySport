package com.znsd.bean;

/**
 * 试卷bean
 * 
 * @author baishui
 *
 */
public class PaperBean {
	private Integer paperId;// 试卷编号
	private String paperName;// 试卷名称
	private String userName;// 发布试卷的用户账号
	private String testEnd;// 考试结束时间
	private String subjectName;// 所属科目
	private StringBuffer anwer;// 试卷内所有题目的编号
	private String testdate;// 考试时长
	private Integer testSource;// 试卷总分

	public Integer getPaperId() {
		return paperId;
	}

	public void setPaperId(Integer paperId) {
		this.paperId = paperId;
	}

	public String getPaperName() {
		return paperName;
	}

	public void setPaperName(String paperName) {
		this.paperName = paperName;
	}

	public String getuserName() {
		return userName;
	}

	public void setuserName(String userName) {
		this.userName = userName;
	}

	public String getTestEnd() {
		return testEnd;
	}

	public void setTestEnd(String testEnd) {
		this.testEnd = testEnd;
	}

	public String getSubjectName() {
		return subjectName;
	}

	public void setSubjectName(String subjectName) {
		this.subjectName = subjectName;
	}

	public StringBuffer getAnwer() {
		return anwer;
	}

	public void setAnwer(StringBuffer anwer) {
		this.anwer = anwer;
	}

	public String getTestdate() {
		return this.testdate;
	}

	public void setTestdate(String testdate) {
		this.testdate = testdate;
	}


	public int getTestSource() {
		return testSource;
	}

	public void setTestSource(int testSource) {
		this.testSource = testSource;
	}

	
	public PaperBean(Integer paperId, String paperName, String userName, String testEnd, String subjectName,
			StringBuffer anwer, String testdate, int testSource) {
		this.paperId = paperId;
		this.paperName = paperName;
		this.userName = userName;
		this.testEnd = testEnd;
		this.subjectName = subjectName;
		this.anwer = anwer;
		this.testdate = testdate;
		this.testSource = testSource;
	}
	
	public PaperBean() {}

	@Override
	public String toString() {
		return "PaperBean [paperId=" + paperId + ", paperName=" + paperName + ", userName=" + userName + ", testEnd="
				+ testEnd + ", subjectName=" + subjectName + ", anwer=" + anwer + ", Testdate=" + testdate
				+ ", testSource=" + testSource + "]";
	}

}
